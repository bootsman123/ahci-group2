<script 
    id="connectionItem"
    type="text/x-jquery-tmpl">  
    {{if connectMethod === 'checkbox'}}
        <input type="checkbox" id="service_{{:service}}" data-connecturl="/auth/{{:service}}/start?callback_type=connect" name="service_{{:service}}" />
    {{/if}}
    {{if isConnected }}
        <div class="socialicon connected pull-left">
            <div class="{{:service}}-image "></div>
            <img src="{{:siteUserImage}}" alt="" />
        </div>
    {{else}}
        <div class="socialicon {{:service}}-icon pull-left"></div>
    {{/if}}

    {{if style === 'verbose'}}
        {{if connectMethod === 'link'}}
            {{for actionBtn tmpl="#buttonTemplate"/}} 
        {{/if}}
    
        {{if connectMethod === 'checkbox'}}
            <label for="service_{{:service}}">
                {{>connectionLabel}}
            </label>
        {{else}}
            <div class='connectionDetails'>
                <p>
                    <span class="site-name">{{>serviceDisplayName}}{{if isConnected}}:{{/if}}</span>
                    {{if isConnected}}
                        <a href="{{:siteUserLink}}" target="_blank">{{:connectionLabel}}</a>
                    {{/if}}
                </p>
                {{if isConnected && autoshare}}
                    <div class='autoShare insetBox'>
                        {{for autoshare tmpl="#checkboxTemplate" /}}
                    </div>
                {{/if}}
            </div>
        {{/if}}            

 
        {{if connectMethod !== 'link'}}
            {{>connectionLabel}}
        {{/if}}
    {{else connectMethod === 'checkbox'}}
        <label for="service_{{:service}}">
            {{>connectionLabel}}
        </label>
    {{else isConnected}}
        {{>connectionLabel}}
    {{else}}
        <a href="/auth/{{:service}}/start?callback_type=connect" class="connectLink" data-connected="{{:isConnected ? 'true' : 'false'}}">
            {{>connectionLabel}}
        </a>
    {{/if}}            
</script>
<script id="checkboxTemplate" type="text/x-jquery-tmpl">
<fieldset class="control-group">
